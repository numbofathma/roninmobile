{
  "version": 2,
  "name": "roninmobile",
  "alias": [
    "www.roninmobile.eu",
    "roninmobile.eu"
  ],
  "builds": [
    { "src": "/static/*", "use": "@now/static" },
    { "src": "package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/project/(?<slug>[^/]+)$", "dest": "/product?slug=$slug" }
  ]
}