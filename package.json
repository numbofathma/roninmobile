{
  "name": "roninmobile",
  "version": "1.1.0",
  "description": "Ronin Mobile website",
  "repository": "https://github.com/numbofathma/roninmobile.git",
  "private": true,
  "dependencies": {
    "embla-carousel": "^8.3.0",
    "embla-carousel-react": "^8.3.0",
    "next": "^15.0.2",
    "react": "^18",
    "react-dom": "^18",
    "resend": "^4.0.0",
    "sharp": "^0.33.5",
    "validator": "^13.12.0"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx|css|scss)\" --write",
    "check": "npm run lint:fix && npm run format && npm run lint && npm run type-check",
    "analyze": "ANALYZE=true npm run build",
    "prepare": "husky || true"
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.15",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-slick": "^0.23.13",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.5.6",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "9.1.6",
    "lint-staged": "15.2.10",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "sass": "^1.80.2",
    "tailwindcss": "^3.4.14",
    "typescript": "5.6.3"
  }
}
